{
    "name" : "hakantaskin/elastic-apm-php-agent",
    "description": "A php agent for Elastic APM for php 5.3 v",
    "license": "MIT",
    "require" : {
        "php" : ">= 5.3",
        "guzzle/guzzle": "~3.9",
        "ramsey/uuid": "2.8.3",
        "ralouphie/getallheaders": "^2",
        "ext-curl" : "*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^4"
    },
    "suggest": {
        "ext-xdebug": "Required for processing of request headers"
    },
    "autoload" : {
        "psr-4" : {
            "HT\\": "src/"
        }
    },
    "autoload-dev": {
    "psr-4": {
        "HT\\Tests\\": "tests/"
    }
  },
    "config" : {
        "optimize-autoloader" : true
    },
    "authors" : [
        {
            "name" : "Hakan Taskin",
            "email" : "mr.hakantaskin@gmail.com",
            "homepage" : "https://github.com/hakantaskin/elastic-apm-agent-php"
        }
    ]
}
